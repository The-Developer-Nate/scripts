		if not l__LocalPlayer__10:GetAttribute("HasVC") then
			if p18.Name == "practice" then
				v173 = "teleport";
			end;
		else
			v173 = "teleport";
		end;
	elseif p18.Name == "practice" then
		v173 = "teleport";
	end;
