			if p13 < v160.Time then
				local v161 = (p13 - v159.Time) / (v160.Time - v159.Time);
				return Color3.new((v160.Value.R - v159.Value.R) * v161 + v159.Value.R, (v160.Value.G - v159.Value.G) * v161 + v159.Value.G, (v160.Value.B - v159.Value.B) * v161 + v159.Value.B);
			end;
		end;
