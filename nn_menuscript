		if l__values__17.servers.socialspace.Value then
			v174 = "return to the main game, where all the stuff is normal again.";
		end;
	end;
	if not (p18:GetAttribute("MapId") and workspace:FindFirstChild(p18:GetAttribute("MapId")) or (p18.Name == "voice" and l__LocalPlayer__10:GetAttribute("HasVC") or p18.Name == "practice")) then
		v173 = "locked";
